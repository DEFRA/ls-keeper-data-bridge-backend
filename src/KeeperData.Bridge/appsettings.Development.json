{
  "DetailedErrors": true,
  "AllowedHosts": "*",
  "AWS": {
    "EMF": {
      "Namespace": "KeeperData/Bridge/Dev",
      "ServiceName": "keeper-data-bridge-dev"
    },
    "Metrics": {
      "MeterName": "KeeperData.Bridge"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:o} [{Level:u4}] ({Application}/{MachineName}/{ThreadId}/{SourceContext}.{Method}) {Message}{NewLine}{Exception}"
        }
      }
    ]
  }
}
