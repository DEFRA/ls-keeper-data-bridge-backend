{
  "AWS": {
    "EMF": {
      "Namespace": "ls-keeper-data-bridge-backend-dev"
    }
  },
  "DetailedErrors": true,
  "AllowedHosts": "*",
  "FeatureFlags": {
    "SourceDataController": true
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:o} [{Level:u4}] ({Application}/{MachineName}/{ThreadId}/{SourceContext}.{Method}) {Message}{NewLine}{Exception}"
        }
      }
    ]
  }
}
