{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "formatter": "Elastic.CommonSchema.Serilog.EcsTextFormatter, Elastic.CommonSchema.Serilog"
        }
      }
    ]
  },
  "Quartz": {
    "Scheduler": {
      "InstanceName": "KeeperDataBridgeScheduler"
    },
    "Jobs": {
      "DataProcessingOrchestrator": {
        "CronSchedule": "0/30 * * * * ?"
      }
    }
  }
}